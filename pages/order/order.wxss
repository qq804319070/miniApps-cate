page {
    position: relative;
}


.layer {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.2);
}

.layer .layer-into {
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
}
.layer .layer-into .head {
    display: flex;
    justify-content: flex-end;
    align-items: center;
    width: 100%;
    height: 74rpx;
    background-color: #f4f4f4;
    padding-right: 25rpx;
    font-size: 26rpx
}

.layer .layer-into .head image {
    width: 24rpx;
    height: 24rpx;
    margin-right: 10rpx;
}


.layer .layer-into .col {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-left: 25rpx;
    padding-right: 20rpx;
    width: 100%;
    height: 100rpx;
    background-color: #ffffff;
    border-bottom: 1px solid #e4e4e4;
}

.layer .layer-into .col .name {
    width: 350rpx;
    color: #666666;
}

.layer .layer-into .col .option {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 150rpx;
    height: 50rpx;
}

.layer .layer-into .col .option view {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    font-size: 38rpx;
}


.col .option .miu {
    border: 1px solid #c4c4c4;

}
.col .option .add {
    background-color: #ffd161;
    color: #000000;

}

