.boundary {
  width: 100%;
  height: 13rpx;
  background-color: #f5f5f5;
}
.nav {
  display: flex;
  width: 100%;
  height: 210rpx;
  padding: 0 33rpx;
  justify-content: space-between;
  align-items: center;
}
.nav view {
  position: relative;
  border-radius: 10rpx;
  overflow: hidden;
  width: 118rpx;
  height: 137rpx;
}
.nav view image {
  width: 100%;
  height: 100%;
}
.storeInfo {
  padding: 0 20rpx;
}
.storeInfo view {
  display: flex;
  align-items: center;
  width: 100%;
  height: 95rpx;
  color: #f9b552;
  font-size: 26rpx;
}
.storeInfo view image {
  width: 48rpx;
  height: 48rpx;
  margin: 0 15rpx;
}
.storeInfo .bot {
  border-bottom: 1px solid #f9b552;
}
.environment-h {
  width: 100%;
  height: 82rpx;
  background-color: #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: center;
}
.environment-h image {
  width: 45rpx;
  height: 45rpx;
}
.environment-h text {
  font-size: 32rpx;
  color: #f9b552;
  margin-left: 18rpx;
}
.environment {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 43rpx;
}
.environment image {
  margin-bottom: 15rpx;
  width: 325rpx;  
  height: 210rpx;
}

.service {
  position: absolute;
  opacity: 0;
  left: 25%;
  top: 25%;
}