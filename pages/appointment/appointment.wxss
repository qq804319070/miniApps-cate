.appoint {
    position: relative;
    padding: 30rpx;
    color: #898989;
    font-size: 26rpx;
    line-height: 55rpx;
    border-bottom: 1px solid #eee;
    background-color: #ffffff;
}

.completed {
    position: absolute;
    right: 40rpx;
    bottom: 30rpx;
    width: 108rpx;
    height: 58rpx;
    border-radius: 10rpx;
    text-align: center;
    line-height: 58rpx;
    border: 1px solid #e64240;
    color: #e64240;
}

.complete {
    position: absolute;
    right: 40rpx;
    bottom: 30rpx;
    color: #fab655;
}

page {
    background-color: #f5f5f5;
}